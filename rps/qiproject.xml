<!-- in qiproject.xml -->
<project version="3">
  <qipython name="rps">
    <!-- this is your executable that will go in bin/ -->
    <script src="app.py" />
    <!-- this is an helper that will go in lib/python2.7/site-packages/ -->
    <module name="helper" />
    <!-- this is a package that will go in lib/python2.7/site-packages/ too -->
    <package name="rps_lib" />
  </qipython>
</project>